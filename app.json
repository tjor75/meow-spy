{
  "expo": {
    "name": "Meow Spy",
    "slug": "meow-spy",
    "version": "1.0.1-alpha",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "sh.now.tjor.meowspy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#111111"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK"
      ],
      "package": "sh.now.tjor.meowspy"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-splash-screen",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/ZenMaruGothic/ZenMaruGothic-Regular.ttf",
            "./assets/fonts/ZenMaruGothic/ZenMaruGothic-Bold.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "ZenMaruGothic-Regular",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/ZenMaruGothic/ZenMaruGothic-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/fonts/ZenMaruGothic/ZenMaruGothic-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          }
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f2f4f422-05cc-492b-8677-2afd8016e8a9"
      }
    }
  }
}
